                ( Nucleus (span 85 107) (rel2par joint)
                  ( Satellite (span 85 86) (rel2par background)
                    ( Nucleus (leaf 85) (rel2par span) (text _!The streets and avenues are planned on a 1 mile by 1 mile grid system ,_!) )
                    ( Satellite (leaf 86) (rel2par manner) (text _!with the main arterials running on each mile ._!) )
                  )
                  ( Nucleus (span 87 107) (rel2par span)
                    ( Nucleus (span 87 93) (rel2par joint)
                      ( Nucleus (span 87 89) (rel2par span)
                        ( Nucleus (leaf 87) (rel2par span) (text _!In the core of the city , named avenues run north / south_!) )
                        ( Satellite (span 88 89) (rel2par elaboration)
                          ( Nucleus (leaf 88) (rel2par span) (text _!and are named after US cities , generally in repeating alphabetical order_!) )
                          ( Satellite (leaf 89) (rel2par elaboration) (text _!( for example , Winston - Yale - Allegheny - Braden ) ._!) )
                        )
                      )
                      ( Satellite (span 90 93) (rel2par elaboration)
                        ( Nucleus (leaf 90) (rel2par joint) (text _!In the mid-town area the names are taken from colleges and college towns ._!) )
                        ( Nucleus (leaf 91) (rel2par joint) (text _!North / South is divided by Admiral Blvd ._!) )
                        ( Nucleus (span 92 93) (rel2par joint)
                          ( Nucleus (leaf 92) (rel2par contrast) (text _!Name streets East of Main are cities east of the Mississippi River ,_!) )
                          ( Nucleus (leaf 93) (rel2par contrast) (text _!vice versa for name streets west of Main ._!) )
                        )
                      )
                    )
                    ( Nucleus (span 94 103) (rel2par joint)
                      ( Satellite (leaf 94) (rel2par background) (text _!In the parts of the city farther from downtown , north - south streets are numbered ._!) )
                      ( Nucleus (span 95 103) (rel2par span)
                        ( Nucleus (span 95 96) (rel2par joint)
                          ( Nucleus (leaf 95) (rel2par span) (text _!It is important to recognize that the specific format of the north - south numbered street names is North / South 145th East / West Avenue ._!) )
                          ( Satellite (leaf 96) (rel2par elaboration) (text _!Numbered streets run East / West with Main Street and the Arkansas River as the dividing line ._!) )
                        )
                        ( Nucleus (leaf 97) (rel2par joint) (text _!Watch out for Place , Street , Avenue designation , e.g. 47th Place , 47th Street , or Florence Place , Florence Avenue ._!) )
                        ( Nucleus (span 98 100) (rel2par joint)
                          ( Nucleus (leaf 98) (rel2par span) (text _!It is important to recognize that the specific format of the east - west numbered street names is West / East 71st Street North / South ._!) )
                          ( Satellite (span 99 100) (rel2par restatement)
                            ( Satellite (leaf 99) (rel2par justify) (text _!In some parts of the city , numbered streets intersect ,_!) )
                            ( Nucleus (leaf 100) (rel2par span) (text _!so the distinction is important ._!) )
                          )
                        )
                        ( Nucleus (span 101 103) (rel2par joint)
                          ( Satellite (leaf 101) (rel2par concession) (text _!Although rare ,_!) )
                          ( Nucleus (span 102 103) (rel2par span)
                            ( Nucleus (leaf 102) (rel2par span) (text _!one east - west numbered street may even intersect with a street of the same number_!) )
                            ( Satellite (leaf 103) (rel2par elaboration) (text _!running north - south ._!) )
                          )
                        )
                      )
                    )
                    ( Nucleus (span 104 107) (rel2par joint)
                      ( Satellite (span 104 106) (rel2par evidence)
                        ( Nucleus (leaf 104) (rel2par sequence) (text _!Downtown streets were originally platted parallel to the Frisco railroad tracks ._!) )
                        ( Nucleus (span 105 106) (rel2par sequence)
                          ( Satellite (leaf 105) (rel2par circumstance) (text _!When Tulsa expanded beyond the bounds of its original plat ,_!) )
                          ( Nucleus (leaf 106) (rel2par span) (text _!the expanded areas were platted in alignment with the points of the compass ._!) )
                        )
                      )
                      ( Nucleus (leaf 107) (rel2par span) (text _!Thus the " twisted " area down - town represents the original extent of Tulsa ca 1907 ._!) )
                    )
