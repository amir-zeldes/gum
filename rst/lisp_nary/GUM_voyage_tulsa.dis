            ( Nucleus (span 117 133) (rel2par joint)
              ( Satellite (leaf 117) (rel2par preparation) (text _!By bicycle_!) )
              ( Nucleus (span 118 133) (rel2par span)
                ( Nucleus (span 118 124) (rel2par joint)
                  ( Nucleus (leaf 118) (rel2par span) (text _!Tulsa has an extensive interconnected paved bike trail system ._!) )
                  ( Satellite (span 119 124) (rel2par elaboration)
                    ( Nucleus (leaf 119) (rel2par joint) (text _!Rivertrail follows the Arkansas River from downtown Tulsa south to the suburbs ._!) )
                    ( Nucleus (leaf 120) (rel2par joint) (text _!The Katy Trail runs west to Sand Springs ._!) )
                    ( Nucleus (span 121 122) (rel2par joint)
                      ( Nucleus (leaf 121) (rel2par span) (text _!The Osage Trail is a rails - to - trails route_!) )
                      ( Satellite (leaf 122) (rel2par elaboration) (text _!that begins at the OSU - Tulsa campus and travels north 15 miles to Skiatook ._!) )
                    )
                    ( Nucleus (span 123 124) (rel2par joint)
                      ( Nucleus (leaf 123) (rel2par joint) (text _!The Creek Trail connects Rivertrail_!) )
                      ( Nucleus (leaf 124) (rel2par joint) (text _!and continues east through Broken Arrow to the NSU - Broken Arrow campus ._!) )
                    )
                  )
                )
                ( Nucleus (span 125 129) (rel2par joint)
                  ( Nucleus (leaf 125) (rel2par joint) (text _!Riders accustomed to flat terrain may find Tulsa 's rolling land to be a bit more challenging , particularly during the heat of summer ._!) )
                  ( Nucleus (span 126 129) (rel2par joint)
                    ( Nucleus (span 126 127) (rel2par contrast)
                      ( Satellite (leaf 126) (rel2par condition) (text _!If you are looking for a good workout ,_!) )
                      ( Nucleus (leaf 127) (rel2par span) (text _!the Creek Turnpike Trail follows the land 's original contours ._!) )
                    )
                    ( Nucleus (span 128 129) (rel2par contrast)
                      ( Nucleus (leaf 128) (rel2par span) (text _!Rivertrail is probably be best choice for the rider_!) )
                      ( Satellite (leaf 129) (rel2par elaboration) (text _!seeking an easy route ._!) )
                    )
                  )
                )
                ( Nucleus (span 130 132) (rel2par joint)
                  ( Nucleus (span 130 131) (rel2par same-unit)
                    ( Nucleus (leaf 130) (rel2par span) (text _!Four bike loan depots ,_!) )
                    ( Satellite (leaf 131) (rel2par elaboration) (text _!located along Rivertrail ,_!) )
                  )
                  ( Nucleus (leaf 132) (rel2par same-unit) (text _!allow riders to borrow a bike for free for up to twenty - four hours ._!) )
                )
                ( Nucleus (leaf 133) (rel2par joint) (text _!Tulsa has an active bicycling community ._!) )
