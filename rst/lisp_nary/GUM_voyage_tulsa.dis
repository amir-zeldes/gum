  ( Nucleus (span 2 133) (rel2par span)
    ( Satellite (span 2 7) (rel2par background)
      ( Nucleus (leaf 2) (rel2par joint) (text _!Tulsa is in the Green Country region of Oklahoma ._!) )
      ( Nucleus (span 3 4) (rel2par joint)
        ( Nucleus (leaf 3) (rel2par joint) (text _!It is also called “ T - town ” by the locals_!) )
        ( Nucleus (leaf 4) (rel2par joint) (text _!and has been called the " Oil Capital of the World " ._!) )
      )
      ( Nucleus (span 5 7) (rel2par joint)
        ( Nucleus (leaf 5) (rel2par joint) (text _!The city had about 391,000 people_!) )
        ( Nucleus (leaf 6) (rel2par joint) (text _!and the metro area had about 937,000 people as of 2010 from the US Census Bureau ._!) )
        ( Nucleus (leaf 7) (rel2par joint) (text _!The Tulsa Bartlesville Combined area had 988,000 in the 2010 census ._!) )
      )
    )
    ( Nucleus (span 8 133) (rel2par span)
      ( Nucleus (span 8 29) (rel2par joint)
        ( Satellite (leaf 8) (rel2par preparation) (text _!Understand_!) )
        ( Nucleus (span 9 29) (rel2par span)
          ( Nucleus (span 9 20) (rel2par joint)
            ( Nucleus (span 9 12) (rel2par span)
              ( Nucleus (span 9 11) (rel2par span)
                ( Nucleus (span 9 10) (rel2par span)
                  ( Nucleus (leaf 9) (rel2par joint) (text _!Tulsa lies in northeastern Oklahoma , at the convergence of the Great Plains and the Ozark Plateau ,_!) )
                  ( Nucleus (leaf 10) (rel2par joint) (text _!and receives an average of 40 inches of precipitation each year ,_!) )
                )
                ( Satellite (leaf 11) (rel2par result) (text _!both of which account for its abundant beautiful rolling green terrain ._!) )
              )
              ( Satellite (leaf 12) (rel2par result) (text _!As a result , Tulsa breaks the Oklahoma stereotype of being nothing but a flat , arid dust bowl ._!) )
            )
            ( Satellite (span 13 20) (rel2par elaboration)
              ( Nucleus (span 13 19) (rel2par span)
                ( Nucleus (leaf 13) (rel2par contrast) (text _!Summers can be very warm_!) )
                ( Nucleus (span 14 19) (rel2par contrast)
                  ( Nucleus (span 14 15) (rel2par span)
                    ( Nucleus (leaf 14) (rel2par span) (text _!and with the cold wind across the plains it can get very cold in the winter ,_!) )
                    ( Satellite (leaf 15) (rel2par concession) (text _!but it does not last long ._!) )
                  )
                  ( Satellite (span 16 19) (rel2par evaluation)
                    ( Nucleus (leaf 16) (rel2par span) (text _!The winters are considered to be very mild ._!) )
                    ( Satellite (span 17 19) (rel2par justify)
                      ( Nucleus (span 17 18) (rel2par span)
                        ( Nucleus (leaf 17) (rel2par span) (text _!There is not much snow ,_!) )
                        ( Satellite (leaf 18) (rel2par elaboration) (text _!just a few inches each year , typically ,_!) )
                      )
                      ( Satellite (leaf 19) (rel2par concession) (text _!although in 2007 and again in 2008 there were rather large " ice storms " ._!) )
                    )
                  )
                )
              )
              ( Satellite (leaf 20) (rel2par elaboration) (text _!Tulsa has over 225 days of sunshine annually ._!) )
            )
          )
          ( Nucleus (leaf 21) (rel2par joint) (text _!In Tulsa you will find old west charm as well as a cosmopolitan atmosphere ._!) )
          ( Nucleus (span 22 24) (rel2par joint)
            ( Nucleus (span 22 23) (rel2par span)
              ( Nucleus (leaf 22) (rel2par contrast) (text _!You will find the people of Tulsa love their city_!) )
              ( Nucleus (leaf 23) (rel2par contrast) (text _!and they have that southern charm ,_!) )
            )
            ( Satellite (leaf 24) (rel2par result) (text _!so they are willing to help you find your way around ._!) )
          )
          ( Nucleus (span 25 29) (rel2par joint)
            ( Nucleus (leaf 25) (rel2par span) (text _!Tulsa has one of the largest concentrations of Art Deco in the nation ,_!) )
            ( Satellite (span 26 29) (rel2par cause)
              ( Nucleus (leaf 26) (rel2par span) (text _!having been a booming city during the 1920s_!) )
              ( Satellite (span 27 29) (rel2par circumstance)
                ( Nucleus (leaf 27) (rel2par span) (text _!when the architecture was first built by rich oil barons_!) )
                ( Satellite (span 28 29) (rel2par elaboration)
                  ( Nucleus (leaf 28) (rel2par joint) (text _!who built stately mansions_!) )
                  ( Nucleus (leaf 29) (rel2par joint) (text _!and turned the Downtown area into a treasure trove of art ._!) )
                )
              )
            )
          )
        )
