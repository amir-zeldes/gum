        ( Nucleus (span 70 133) (rel2par span)
          ( Satellite (span 70 75) (rel2par background)
            ( Nucleus (leaf 70) (rel2par span) (text _!Thanks to urban planning , the major city streets are placed in a grid layout ._!) )
            ( Satellite (span 71 75) (rel2par elaboration)
              ( Nucleus (span 71 73) (rel2par contrast)
                ( Nucleus (leaf 71) (rel2par span) (text _!Almost all major intersections are one mile from each other , and exactly in a straight line ._!) )
                ( Satellite (span 72 73) (rel2par result)
                  ( Nucleus (leaf 72) (rel2par span) (text _!That makes it much easier to find places than in cities_!) )
                  ( Satellite (leaf 73) (rel2par elaboration) (text _!where streets go every which way ._!) )
                )
              )
              ( Nucleus (span 74 75) (rel2par contrast)
                ( Nucleus (leaf 74) (rel2par span) (text _!The major exception is downtown ,_!) )
                ( Satellite (leaf 75) (rel2par elaboration) (text _!which is slanted at almost a 45 degree angle to the rest of the grid ._!) )
