                  ( Nucleus (span 77 78) (rel2par span)
                    ( Nucleus (leaf 77) (rel2par span) (text _!Several freeways and bypasses can be used_!) )
                    ( Satellite (leaf 78) (rel2par purpose) (text _!to easily get around the Tulsa Metro area :_!) )
                  )
                  ( Satellite (span 79 84) (rel2par elaboration)
                    ( Nucleus (leaf 79) (rel2par same-unit) (text _!I - 244 , I - 44 , US 169_!) )
                    ( Nucleus (span 80 81) (rel2par same-unit)
                      ( Nucleus (leaf 80) (rel2par span) (text _!( Mingo Valley Expressway ,_!) )
                      ( Satellite (leaf 81) (rel2par restatement) (text _!aka " Pearl Harbor Memorial Expressway " ) ,_!) )
                    )
                    ( Nucleus (span 82 83) (rel2par same-unit)
                      ( Nucleus (leaf 82) (rel2par span) (text _!US 75 , Hwy 51_!) )
                      ( Satellite (leaf 83) (rel2par restatement) (text _!( Broken Arrow Expressway , The " B.A. " ) ,_!) )
                    )
                    ( Nucleus (leaf 84) (rel2par same-unit) (text _!Creek Turnpike ._!) )
                  )
