( Root (span 1 99) 
  ( Satellite (span 1 5) (rel2par organization-preparation)
    ( Nucleus (leaf 1) (rel2par span) (text _!It is my special pleasure to address the fifty - ninth session of the General Assembly of the United Nations ._!) )
    ( Satellite (span 2 5) (rel2par context-background)
      ( Nucleus (leaf 2) (rel2par span) (text _!This universal forum has enabled the peoples of the world to coordinate and harmonize their efforts_!) )
      ( Satellite (span 3 5) (rel2par purpose-attribute)
        ( Nucleus (leaf 3) (rel2par joint-list) (text _!in preserving peace and security ,_!) )
        ( Nucleus (leaf 4) (rel2par joint-list) (text _!in achieving prosperity_!) )
        ( Nucleus (leaf 5) (rel2par joint-list) (text _!and upholding the values of human civilization ._!) )
      )
    )
  )
  ( Nucleus (span 6 99) (rel2par span)
    ( Satellite (span 6 10) (rel2par organization-preparation)
      ( Nucleus (span 6 8) (rel2par joint-other)
        ( Nucleus (span 6 7) (rel2par joint-list)
          ( Nucleus (leaf 6) (rel2par span) (text _!I would like to congratulate you , Mr. President ,_!) )
          ( Satellite (leaf 7) (rel2par causal-cause) (text _!on being elected to preside over this august body_!) )
        )
        ( Nucleus (leaf 8) (rel2par joint-list) (text _!and to express my confidence in your successful leadership ._!) )
      )
      ( Nucleus (span 9 10) (rel2par joint-other)
        ( Nucleus (leaf 9) (rel2par span) (text _!I would also like , on behalf of Albania , to convey to the Secretary - General our highest appreciation for his leading contribution to the fulfilment of the historical mission of the United Nations and to the aspirations of our peoples for a better world_!) )
        ( Satellite (leaf 10) (rel2par elaboration-attribute) (text _!in which only peace , democracy and economic prosperity reign ._!) )
      )
    )
    ( Nucleus (span 11 99) (rel2par span)
      ( Nucleus (span 11 21) (rel2par joint-other)
        ( Satellite (span 11 15) (rel2par context-background)
          ( Satellite (leaf 11) (rel2par evaluation-comment) (text _!This session of the General Assembly takes place in a milestone year for Europe ._!) )
          ( Nucleus (span 12 15) (rel2par span)
            ( Satellite (leaf 12) (rel2par evaluation-comment) (text _!Only a few months ago , a historic event vitalized the dream of a more united and stronger Europe ._!) )
            ( Nucleus (span 13 15) (rel2par span)
              ( Nucleus (span 13 14) (rel2par same-unit)
                ( Nucleus (leaf 13) (rel2par span) (text _!Ten European countries ,_!) )
                ( Satellite (leaf 14) (rel2par elaboration-attribute) (text _!sharing a common aspiration for integration ,_!) )
              )
              ( Nucleus (leaf 15) (rel2par same-unit) (text _!were admitted into the European Union ._!) )
            )
          )
        )
        ( Nucleus (span 16 21) (rel2par span)
          ( Nucleus (span 16 17) (rel2par span)
            ( Nucleus (leaf 16) (rel2par span) (text _!The Albanian Government and society are engaged in an all - round European integration process ,_!) )
            ( Satellite (leaf 17) (rel2par explanation-justify) (text _!and they are carrying out necessary reforms with the necessary political will and drive ._!) )
          )
          ( Satellite (span 18 21) (rel2par elaboration-additional)
            ( Nucleus (span 18 19) (rel2par span)
              ( Nucleus (leaf 18) (rel2par span) (text _!Albania is working to strengthen the democratic institutions and capacities of its central and local government_!) )
              ( Satellite (leaf 19) (rel2par purpose-goal) (text _!with a view to bringing its legislation and conditions into line with European Union standards ,_!) )
            )
            ( Satellite (span 20 21) (rel2par explanation-justify)
              ( Nucleus (leaf 20) (rel2par span) (text _!progressively achieving concrete and measurable results ,_!) )
              ( Satellite (leaf 21) (rel2par mode-manner) (text _!as previously defined ._!) )
            )
          )
        )
      )
      ( Nucleus (span 22 33) (rel2par joint-other)
        ( Nucleus (span 22 29) (rel2par joint-other)
          ( Satellite (leaf 22) (rel2par causal-cause) (text _!Determined to make our contribution to regional and global security ,_!) )
          ( Nucleus (span 23 29) (rel2par span)
            ( Nucleus (leaf 23) (rel2par span) (text _!the Albanian Government considers the country 's integration into NATO to be one of its major objectives ._!) )
            ( Satellite (span 24 29) (rel2par explanation-evidence)
              ( Nucleus (span 24 26) (rel2par span)
                ( Nucleus (span 24 25) (rel2par joint-list)
                  ( Nucleus (leaf 24) (rel2par span) (text _!The NATO Istanbul Summit last June commended the progress_!) )
                  ( Satellite (leaf 25) (rel2par elaboration-attribute) (text _!made by my country in this regard_!) )
                )
                ( Nucleus (leaf 26) (rel2par joint-list) (text _!and encouraged the deepening of reforms ._!) )
              )
              ( Satellite (span 27 29) (rel2par elaboration-additional)
                ( Nucleus (leaf 27) (rel2par joint-list) (text _!It welcomed Albania 's contribution to regional stability and cooperation_!) )
                ( Nucleus (span 28 29) (rel2par joint-list)
                  ( Nucleus (leaf 28) (rel2par span) (text _!and committed to assess our further progress at the next NATO summit ,_!) )
                  ( Satellite (leaf 29) (rel2par elaboration-attribute) (text _!which would hopefully mark the start of negotiations for our full membership in the alliance ._!) )
                )
              )
            )
          )
        )
        ( Nucleus (span 30 33) (rel2par joint-other)
          ( Satellite (leaf 30) (rel2par attribution-positive) (text _!We believe_!) )
          ( Nucleus (span 31 33) (rel2par span)
            ( Nucleus (span 31 32) (rel2par same-unit)
              ( Nucleus (leaf 31) (rel2par span) (text _!that the commitments_!) )
              ( Satellite (leaf 32) (rel2par elaboration-attribute) (text _!set up in the framework of the Adriatic Charter_!) )
            )
            ( Nucleus (leaf 33) (rel2par same-unit) (text _!are also contributing to meet NATO standards , to the benefit of regional stability and security ._!) )
          )
        )
      )
      ( Nucleus (span 34 39) (rel2par joint-other)
        ( Satellite (span 34 35) (rel2par context-background)
          ( Satellite (leaf 34) (rel2par attribution-positive) (text _!We remain truly convinced_!) )
          ( Nucleus (leaf 35) (rel2par span) (text _!that the respect and protection of human rights and freedoms are the main pillars of a pluralist democratic society ._!) )
        )
        ( Nucleus (span 36 39) (rel2par span)
          ( Nucleus (span 36 38) (rel2par joint-other)
            ( Nucleus (leaf 36) (rel2par span) (text _!The Government of the Republic of Albania will continue to constantly demonstrate its commitment to a full implementation of the standards_!) )
            ( Satellite (span 37 38) (rel2par elaboration-attribute)
              ( Nucleus (leaf 37) (rel2par span) (text _!enshrined in the United Nations and Council of Europe conventions , as well as the relevant documents of regional organizations , such as the Organization for Security and Cooperation in Europe ,_!) )
              ( Satellite (leaf 38) (rel2par elaboration-attribute) (text _!in which we are playing a very active role ._!) )
            )
          )
          ( Nucleus (leaf 39) (rel2par joint-other) (text _!We will continue to do so in the future as well ._!) )
        )
      )
      ( Nucleus (span 40 44) (rel2par joint-other)
        ( Satellite (span 40 41) (rel2par mode-means)
          ( Nucleus (leaf 40) (rel2par joint-list) (text _!By adopting contemporary legislation_!) )
          ( Nucleus (leaf 41) (rel2par joint-list) (text _!and by becoming a party to other recently adopted international instruments on human rights ,_!) )
        )
        ( Nucleus (span 42 44) (rel2par span)
          ( Nucleus (leaf 42) (rel2par span) (text _!Albania has associated itself with countries_!) )
          ( Satellite (span 43 44) (rel2par elaboration-attribute)
            ( Nucleus (leaf 43) (rel2par span) (text _!that are enforcing a number of national strategies and programmes_!) )
            ( Satellite (leaf 44) (rel2par elaboration-attribute) (text _!focusing on specific population groups , especially women , children , persons in need and minorities , among others ._!) )
          )
        )
      )
      ( Nucleus (span 45 57) (rel2par joint-other)
        ( Nucleus (span 45 55) (rel2par span)
          ( Nucleus (span 45 48) (rel2par joint-other)
            ( Satellite (span 45 46) (rel2par explanation-evidence)
              ( Nucleus (leaf 45) (rel2par joint-list) (text _!In recent years , the Albanian Government has started to periodically submit reports to the committees of the six main United Nations human rights treaties_!) )
              ( Nucleus (leaf 46) (rel2par joint-list) (text _!and is taking their recommendations into serious consideration ._!) )
            )
            ( Nucleus (span 47 48) (rel2par span)
              ( Nucleus (leaf 47) (rel2par span) (text _!It is our belief that these international mechanisms play a significant and important role as guardians of the common human values_!) )
              ( Satellite (leaf 48) (rel2par elaboration-attribute) (text _!we have together adopted ._!) )
            )
          )
          ( Nucleus (span 49 55) (rel2par joint-other)
            ( Nucleus (span 49 50) (rel2par span)
              ( Nucleus (leaf 49) (rel2par span) (text _!Albania , as one of the 191 signatory countries of the Millennium Declaration , remains fully committed to implementation of the Millennium Development Goals ,_!) )
              ( Satellite (leaf 50) (rel2par elaboration-attribute) (text _!which have been properly reflected in a national strategy for social and economic development ._!) )
            )
            ( Satellite (span 51 55) (rel2par explanation-evidence)
              ( Nucleus (span 51 52) (rel2par same-unit)
                ( Nucleus (leaf 51) (rel2par span) (text _!The pattern of our sustained economic growth ,_!) )
                ( Satellite (leaf 52) (rel2par elaboration-attribute) (text _!which is at 6 to 7 per cent for the seventh consecutive year ,_!) )
              )
              ( Nucleus (span 53 55) (rel2par same-unit)
                ( Nucleus (leaf 53) (rel2par span) (text _!is a good support base for the whole society_!) )
                ( Satellite (span 54 55) (rel2par purpose-goal)
                  ( Nucleus (leaf 54) (rel2par joint-list) (text _!to maintain higher standards of respect for human rights_!) )
                  ( Nucleus (leaf 55) (rel2par joint-list) (text _!and to fight poverty and crime ._!) )
                )
              )
            )
          )
        )
        ( Satellite (span 56 57) (rel2par evaluation-comment)
          ( Nucleus (leaf 56) (rel2par span) (text _!I would like to take this opportunity_!) )
          ( Satellite (leaf 57) (rel2par purpose-attribute) (text _!to thank the United Nations specialized agencies for their valuable assistance in support of the implementation of these objectives nationwide and on a central and local level ._!) )
        )
      )
      ( Nucleus (span 58 76) (rel2par joint-other)
        ( Nucleus (span 58 63) (rel2par joint-other)
          ( Nucleus (span 58 59) (rel2par span)
            ( Satellite (leaf 58) (rel2par attribution-positive) (text _!The Albanian Government is pleased to note_!) )
            ( Nucleus (leaf 59) (rel2par span) (text _!that an ever - healthier climate of confidence and relations of bilateral and multilateral cooperation are prevailing in the South - Eastern European region ._!) )
          )
          ( Satellite (span 60 63) (rel2par explanation-justify)
            ( Nucleus (leaf 60) (rel2par span) (text _!The countries of the region are fully engaged in a process_!) )
            ( Satellite (span 61 63) (rel2par elaboration-attribute)
              ( Nucleus (leaf 61) (rel2par joint-list) (text _!that will lead us clearly toward Euro - Atlantic integration_!) )
              ( Nucleus (span 62 63) (rel2par joint-list)
                ( Nucleus (leaf 62) (rel2par span) (text _!and at the same time continually remove us from the extremist tendencies_!) )
                ( Satellite (leaf 63) (rel2par elaboration-attribute) (text _!that caused a number of conflicts in the last decade ._!) )
              )
            )
          )
        )
        ( Nucleus (span 64 76) (rel2par joint-other)
          ( Satellite (span 64 66) (rel2par context-background)
            ( Satellite (leaf 64) (rel2par organization-preparation) (text _!The regional policy of my Government is guided by the motto :_!) )
            ( Nucleus (span 65 66) (rel2par span)
              ( Satellite (leaf 65) (rel2par contingency-condition) (text _!' the more integrated in the region ,_!) )
              ( Nucleus (leaf 66) (rel2par span) (text _!the more integrated in Europe . '_!) )
            )
          )
          ( Nucleus (span 67 76) (rel2par span)
            ( Nucleus (span 67 74) (rel2par joint-other)
              ( Nucleus (span 67 73) (rel2par span)
                ( Satellite (leaf 67) (rel2par context-circumstance) (text _!In implementing this policy ,_!) )
                ( Nucleus (span 68 73) (rel2par span)
                  ( Nucleus (span 68 70) (rel2par span)
                    ( Nucleus (span 68 69) (rel2par same-unit)
                      ( Nucleus (leaf 68) (rel2par span) (text _!Albania is cooperating_!) )
                      ( Satellite (leaf 69) (rel2par mode-manner) (text _!as never before_!) )
                    )
                    ( Nucleus (leaf 70) (rel2par same-unit) (text _!with all the countries of the region_!) )
                  )
                  ( Satellite (span 71 73) (rel2par mode-means)
                    ( Nucleus (leaf 71) (rel2par joint-list) (text _!by making borders less relevant ,_!) )
                    ( Nucleus (leaf 72) (rel2par joint-list) (text _!opening up to one regional market the implementation of free trade agreements and free movement corridors ,_!) )
                    ( Nucleus (leaf 73) (rel2par joint-list) (text _!and stimulating foreign direct investment ._!) )
                  )
                )
              )
              ( Satellite (leaf 74) (rel2par explanation-evidence) (text _!We are especially cooperating with the common fight against organized crime and all kinds of illegal trafficking ._!) )
            )
            ( Nucleus (span 75 76) (rel2par joint-other)
              ( Nucleus (leaf 75) (rel2par span) (text _!In the meantime , Albania is reinforcing its border management and control ,_!) )
              ( Satellite (leaf 76) (rel2par mode-manner) (text _!guided by the European Union and NATO standards ._!) )
            )
          )
        )
      )
      ( Nucleus (span 77 86) (rel2par joint-other)
        ( Satellite (leaf 77) (rel2par context-background) (text _!My country will hold general parliamentary elections by the middle of next year ._!) )
        ( Nucleus (span 78 86) (rel2par span)
          ( Nucleus (span 78 85) (rel2par span)
            ( Nucleus (span 78 81) (rel2par span)
              ( Nucleus (span 78 79) (rel2par span)
                ( Nucleus (leaf 78) (rel2par span) (text _!We are committed to doing all_!) )
                ( Satellite (leaf 79) (rel2par elaboration-attribute) (text _!that is required_!) )
              )
              ( Satellite (span 80 81) (rel2par purpose-goal)
                ( Satellite (leaf 80) (rel2par attribution-positive) (text _!to ensure_!) )
                ( Nucleus (leaf 81) (rel2par span) (text _!that the election process is fully compatible with international standards for free and fair elections ._!) )
              )
            )
            ( Satellite (span 82 85) (rel2par elaboration-additional)
              ( Nucleus (leaf 82) (rel2par span) (text _!To that end , we are committed to continuing to work with relevant specialized institutions on elections_!) )
              ( Satellite (span 83 85) (rel2par mode-means)
                ( Nucleus (span 83 84) (rel2par same-unit)
                  ( Nucleus (leaf 83) (rel2par span) (text _!by fully implementing especially OSCE / Office for Democratic Institutions and Human Rights_!) )
                  ( Satellite (leaf 84) (rel2par restatement-partial) (text _!( ODIHR )_!) )
                )
                ( Nucleus (leaf 85) (rel2par same-unit) (text _!recommendations ._!) )
              )
            )
          )
          ( Satellite (leaf 86) (rel2par evaluation-comment) (text _!For us , the process and standards are more important than the results of the elections ._!) )
        )
      )
      ( Nucleus (span 87 93) (rel2par joint-other)
        ( Nucleus (span 87 88) (rel2par same-unit)
          ( Nucleus (leaf 87) (rel2par span) (text _!In the context_!) )
          ( Satellite (leaf 88) (rel2par elaboration-attribute) (text _!of strengthening regional cooperation ,_!) )
        )
        ( Nucleus (span 89 93) (rel2par same-unit)
          ( Nucleus (leaf 89) (rel2par span) (text _!my country is committed to a more enhanced partnership with all regional organizations and initiatives_!) )
          ( Satellite (span 90 93) (rel2par elaboration-attribute)
            ( Nucleus (leaf 90) (rel2par joint-list) (text _!that serve the promotion of good neighbourly relations ;_!) )
            ( Nucleus (leaf 91) (rel2par joint-list) (text _!the strengthening of regional peace , security and stability ;_!) )
            ( Nucleus (leaf 92) (rel2par joint-list) (text _!ensuring political support for integration into Euro - Atlantic structures ,_!) )
            ( Nucleus (leaf 93) (rel2par joint-list) (text _!and attracting financing for national and regional projects in priority fields such as energy , transportation and telecommunications ._!) )
          )
        )
      )
      ( Nucleus (span 94 99) (rel2par joint-other)
        ( Nucleus (span 94 97) (rel2par joint-other)
          ( Nucleus (leaf 94) (rel2par span) (text _!On its path towards European integration , the South - East European region is still facing pending challenges , such as the future of Kosovo ._!) )
          ( Satellite (span 95 97) (rel2par evaluation-comment)
            ( Nucleus (leaf 95) (rel2par span) (text _!Albania greatly appreciates the recent democratic and integration developments in Kosovo ,_!) )
            ( Satellite (span 96 97) (rel2par causal-cause)
              ( Nucleus (leaf 96) (rel2par span) (text _!thanks to the efforts of the respective peoples and the partnership of its self - governing institutions with the United Nations Interim Administration Mission in Kosovo_!) )
              ( Satellite (leaf 97) (rel2par restatement-partial) (text _!( UNMIK ) ._!) )
            )
          )
        )
        ( Nucleus (span 98 99) (rel2par joint-other)
          ( Nucleus (leaf 98) (rel2par span) (text _!Allow me at this point to congratulate Mr. Soren Jessen - Petersen , Special Representative for Kosovo of the United Nations Secretary - General ,_!) )
          ( Satellite (leaf 99) (rel2par context-circumstance) (text _!in assuming this very important duty ._!) )
        )
      )
    )
  )
)
