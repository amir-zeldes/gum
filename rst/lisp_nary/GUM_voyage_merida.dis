              ( Nucleus (span 16 17) (rel2par span)
                ( Nucleus (leaf 16) (rel2par span) (text _!The city was founded by the Spanish conquistadors in the 1540s on top of a centuries - old Maya city_!) )
                ( Satellite (leaf 17) (rel2par elaboration) (text _!called T'ho ._!) )
              )
              ( Satellite (leaf 18) (rel2par elaboration) (text _!The palatial home of the family of conquistador leader Montejo can still be seen on the south side of the Zócalo or main square ._!) )
            )
            ( Satellite (span 19 20) (rel2par elaboration)
              ( Nucleus (leaf 19) (rel2par span) (text _!Here and there bits of ancient Maya stonework can be seen_!) )
              ( Satellite (leaf 20) (rel2par elaboration) (text _!reused in Spanish Colonial era buildings in the old part of town ._!) )
            )
          )
          ( Nucleus (span 21 26) (rel2par sequence)
            ( Nucleus (span 21 23) (rel2par span)
              ( Nucleus (leaf 21) (rel2par span) (text _!From the later 1800s to the 1920s Mérida enjoyed prosperous boom times_!) )
              ( Satellite (span 22 23) (rel2par cause)
                ( Nucleus (leaf 22) (rel2par span) (text _!fueled by the henequen or sisal plant harvest ,_!) )
                ( Satellite (leaf 23) (rel2par result) (text _!which made Yucatán the rope maker to the world ._!) )
              )
            )
            ( Satellite (span 24 26) (rel2par result)
              ( Nucleus (leaf 24) (rel2par joint) (text _!Progressive Mérida had electric trams and street lights before Mexico City ._!) )
              ( Nucleus (span 25 26) (rel2par joint)
                ( Nucleus (leaf 25) (rel2par span) (text _!The wealthy constructed the grand Pasejo Montejo avenue north of the old town ,_!) )
                ( Satellite (leaf 26) (rel2par elaboration) (text _!inspired by the Champs - Élysées in Paris ._!) )
              )
            )
          )
          ( Nucleus (span 27 29) (rel2par sequence)
            ( Nucleus (leaf 27) (rel2par span) (text _!With the development of artificial twines the sisal boom ended ,_!) )
            ( Satellite (span 28 29) (rel2par result)
              ( Nucleus (leaf 28) (rel2par span) (text _!and Mérida slowed to a more sleepy provincial capital_!) )
              ( Satellite (leaf 29) (rel2par circumstance) (text _!until development picked back up in the late 20th century ._!) )
            )
