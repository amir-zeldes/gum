          ( Nucleus (span 8 12) (rel2par joint)
            ( Nucleus (leaf 8) (rel2par span) (text _!Mérida is a city of contrasts ._!) )
            ( Satellite (span 9 12) (rel2par evidence)
              ( Nucleus (leaf 9) (rel2par joint) (text _!You will find elegant hotels , restaurants and malls in the northern part of the city ._!) )
              ( Nucleus (span 10 11) (rel2par joint)
                ( Nucleus (leaf 10) (rel2par span) (text _!Downtown , there are hotels and restaurants_!) )
                ( Satellite (leaf 11) (rel2par purpose) (text _!to suit every budget ._!) )
              )
              ( Nucleus (leaf 12) (rel2par joint) (text _!A large central market and numerous small shops are found all around the main plaza ._!) )
            )
          )
          ( Nucleus (span 13 15) (rel2par joint)
            ( Nucleus (span 13 14) (rel2par span)
              ( Nucleus (leaf 13) (rel2par span) (text _!Mérida has a rich cultural life_!) )
              ( Satellite (leaf 14) (rel2par evaluation) (text _!which also reflects its diversity ._!) )
