            ( Nucleus (span 75 77) (rel2par joint-list)
              ( Nucleus (span 75 76) (rel2par joint-list)
                ( Satellite (leaf 75) (rel2par attribution-negative) (text _!I did not say_!) )
                ( Nucleus (leaf 76) (rel2par span) (text _!we would not have new pressures and new problems ._!) )
              )
              ( Nucleus (leaf 77) (rel2par joint-list) (text _!Nor did I speak of swift solutions in 100 days in office ._!) )
            )
          )
          ( Nucleus (span 78 82) (rel2par span)
            ( Nucleus (span 78 80) (rel2par same-unit)
              ( Nucleus (leaf 78) (rel2par span) (text _!I talked instead about the kind of America_!) )
              ( Satellite (span 79 80) (rel2par elaboration-attribute)
                ( Nucleus (leaf 79) (rel2par span) (text _!that I wanted for my family and for your family and all those_!) )
                ( Satellite (leaf 80) (rel2par elaboration-attribute) (text _!who are citizens of this country in these difficult and changing years ,_!) )
