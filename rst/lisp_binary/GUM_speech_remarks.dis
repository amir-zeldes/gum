( Root (span 1 120) 
  ( Nucleus (span 1 40) (rel2par joint-other)
    ( Nucleus (span 1 2) (rel2par span)
      ( Nucleus (leaf 1) (rel2par joint-list) (text _!I am proud to come here to Philadelphia_!) )
      ( Nucleus (leaf 2) (rel2par joint-list) (text _!and join my fellow Democrats ._!) )
    )
    ( Satellite (span 3 40) (rel2par elaboration-additional)
      ( Nucleus (span 3 10) (rel2par joint-list)
        ( Nucleus (span 3 6) (rel2par joint-list)
          ( Satellite (span 3 5) (rel2par causal-cause)
            ( Nucleus (leaf 3) (rel2par span) (text _!In 1960 , the Democrats of this city produced a margin in the presidential race_!) )
            ( Satellite (span 4 5) (rel2par elaboration-additional)
              ( Nucleus (leaf 4) (rel2par span) (text _!that was in this city three times as large_!) )
              ( Satellite (leaf 5) (rel2par context-circumstance) (text _!as it was in the whole United States ._!) )
            )
          )
          ( Nucleus (leaf 6) (rel2par span) (text _!So I am proud to be back here again ,_!) )
        )
        ( Nucleus (span 7 10) (rel2par joint-list)
          ( Nucleus (leaf 7) (rel2par span) (text _!and I am very happy to be introduced by your distinguished chairman , Bill Green ._!) )
          ( Satellite (span 8 10) (rel2par context-background)
            ( Satellite (leaf 8) (rel2par context-circumstance) (text _!When he became chairman of this city committee ,_!) )
            ( Nucleus (span 9 10) (rel2par span)
              ( Nucleus (leaf 9) (rel2par joint-list) (text _!there were 300,000 more Republicans registered in the city of Philadelphia than Democrats ,_!) )
              ( Nucleus (leaf 10) (rel2par joint-list) (text _!and it is a source of satisfaction to me that tonight there are 260,000 more Democrats registered ._!) )
            )
          )
        )
      )
      ( Nucleus (span 11 40) (rel2par joint-list)
        ( Nucleus (span 11 38) (rel2par joint-list)
          ( Nucleus (span 11 18) (rel2par joint-list)
