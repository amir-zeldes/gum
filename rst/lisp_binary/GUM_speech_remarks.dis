                    ( Nucleus (leaf 105) (rel2par span) (text _!And our high wartime tax rates still prevent our economy from growing as fully and as freely_!) )
                    ( Satellite (leaf 106) (rel2par mode-manner) (text _!as it must ._!) )
                  )
                )
              )
              ( Nucleus (span 107 120) (rel2par span)
                ( Nucleus (span 107 108) (rel2par joint-list)
                  ( Nucleus (leaf 107) (rel2par span) (text _!But one fact is that 2 1/2 million people more are working in the United States_!) )
                  ( Satellite (leaf 108) (rel2par adversative-antithesis) (text _!than were working 33 months ago ._!) )
                )
