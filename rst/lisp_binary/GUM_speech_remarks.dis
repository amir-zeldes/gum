            ( Nucleus (span 68 72) (rel2par joint-list)
              ( Nucleus (span 68 69) (rel2par joint-list)
                ( Nucleus (leaf 68) (rel2par span) (text _!and also to express appreciation for the help_!) )
                ( Satellite (leaf 69) (rel2par elaboration-attribute) (text _!we received from those Democrats in this city and State ,_!) )
              )
              ( Nucleus (span 70 72) (rel2par joint-list)
                ( Nucleus (leaf 70) (rel2par span) (text _!and to report to you on the progress_!) )
                ( Satellite (span 71 72) (rel2par elaboration-attribute)
