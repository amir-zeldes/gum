                ( Nucleus (leaf 102) (rel2par span) (text _!That goal has not been fully achieved ._!) )
                ( Satellite (span 103 106) (rel2par explanation-evidence)
                  ( Nucleus (span 103 104) (rel2par joint-other)
                    ( Nucleus (leaf 103) (rel2par span) (text _!There are still too many men and women , particularly young men and women ,_!) )
                    ( Satellite (leaf 104) (rel2par elaboration-attribute) (text _!unable to find work ._!) )
                  )
