              ( Nucleus (span 15 18) (rel2par same-unit)
                ( Nucleus (span 15 17) (rel2par same-unit)
                  ( Nucleus (leaf 15) (rel2par span) (text _!as one_!) )
                  ( Satellite (span 16 17) (rel2par elaboration-attribute)
                    ( Satellite (leaf 16) (rel2par attribution-positive) (text _!who believes_!) )
                    ( Nucleus (leaf 17) (rel2par span) (text _!that the Democratic Party has meant progress for this city , this State , and this country ,_!) )
                  )
                )
                ( Nucleus (leaf 18) (rel2par same-unit) (text _!I am proud to be here in Philadelphia ._!) )
              )
            )
          )
          ( Nucleus (span 19 38) (rel2par joint-list)
            ( Nucleus (span 19 28) (rel2par span)
              ( Nucleus (span 19 25) (rel2par span)
                ( Nucleus (leaf 19) (rel2par span) (text _!And I am proud to be here with your mayor ._!) )
                ( Satellite (span 20 25) (rel2par elaboration-additional)
                  ( Satellite (span 20 21) (rel2par adversative-concession)
                    ( Nucleus (leaf 20) (rel2par joint-list) (text _!I do not come from Philadelphia ,_!) )
                    ( Nucleus (leaf 21) (rel2par joint-list) (text _!and I would not interfere ,_!) )
                  )
                  ( Nucleus (span 22 25) (rel2par span)
                    ( Satellite (leaf 22) (rel2par attribution-positive) (text _!but I am hopeful and confident_!) )
                    ( Nucleus (span 23 25) (rel2par span)
                      ( Nucleus (leaf 23) (rel2par same-unit) (text _!that_!) )
                      ( Nucleus (span 24 25) (rel2par same-unit)
                        ( Satellite (leaf 24) (rel2par context-circumstance) (text _!when we come to the Army - Navy game in a month from now ,_!) )
                        ( Nucleus (leaf 25) (rel2par span) (text _!we will be greeted by the new mayor of Philadelphia , Mayor Tale ._!) )
                      )
                    )
                  )
                )
              )
              ( Satellite (span 26 28) (rel2par evaluation-comment)
                ( Satellite (span 26 27) (rel2par adversative-concession)
                  ( Satellite (leaf 26) (rel2par causal-cause) (text _!I have n't given a political speech for about 3 years ,_!) )
                  ( Nucleus (leaf 27) (rel2par span) (text _!so I am a little out of practice ,_!) )
                )
                ( Nucleus (leaf 28) (rel2par span) (text _!but I am gradually getting back into it ._!) )
              )
            )
            ( Satellite (span 29 38) (rel2par evaluation-comment)
              ( Nucleus (leaf 29) (rel2par span) (text _!And I am glad to be here with a mayor_!) )
              ( Satellite (span 30 38) (rel2par elaboration-attribute)
                ( Nucleus (span 30 36) (rel2par span)
                  ( Nucleus (span 30 31) (rel2par span)
                    ( Nucleus (leaf 30) (rel2par joint-list) (text _!who follows two other distinguished mayors of this city ,_!) )
                    ( Nucleus (leaf 31) (rel2par joint-list) (text _!who carries on their tradition_!) )
                  )
                  ( Satellite (span 32 36) (rel2par elaboration-additional)
                    ( Nucleus (span 32 33) (rel2par same-unit)
                      ( Nucleus (leaf 32) (rel2par span) (text _!- Joe Clark , your United States Senator_!) )
                      ( Satellite (leaf 33) (rel2par elaboration-attribute) (text _!who was a great mayor of Philadelphia ,_!) )
                    )
                    ( Nucleus (span 34 36) (rel2par same-unit)
                      ( Nucleus (leaf 34) (rel2par span) (text _!and Dick Dilworth ,_!) )
                      ( Satellite (span 35 36) (rel2par elaboration-attribute)
                        ( Nucleus (leaf 35) (rel2par joint-list) (text _!who followed him ,_!) )
                        ( Nucleus (leaf 36) (rel2par joint-list) (text _!who was a great mayor of Philadelphia ._!) )
                      )
                    )
                  )
                )
                ( Satellite (span 37 38) (rel2par elaboration-additional)
                  ( Nucleus (leaf 37) (rel2par span) (text _!That is the tradition of honest , progressive democratic government_!) )
                  ( Satellite (leaf 38) (rel2par elaboration-attribute) (text _!that Mayor Tate carries on ._!) )
                )
              )
            )
          )
        )
        ( Nucleus (span 39 40) (rel2par joint-list)
          ( Nucleus (leaf 39) (rel2par span) (text _!And I am glad to be here with Judge Hoffman and Senator Mahady_!) )
          ( Satellite (leaf 40) (rel2par elaboration-attribute) (text _!who also run this fall in the State of Pennsylvania ._!) )
        )
      )
    )
