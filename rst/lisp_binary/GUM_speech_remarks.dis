                ( Nucleus (span 92 93) (rel2par joint-list)
                  ( Satellite (leaf 92) (rel2par attribution-positive) (text _!and I am still determined_!) )
                  ( Nucleus (leaf 93) (rel2par span) (text _!that this Nation shall continue to strive to meet those goals ._!) )
                )
                ( Nucleus (leaf 94) (rel2par joint-list) (text _!And I am gratified to be able to report some progress in the last 33 1/2 months ._!) )
              )
            )
