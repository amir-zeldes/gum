            ( Nucleus (span 81 82) (rel2par same-unit)
              ( Nucleus (leaf 81) (rel2par span) (text _!the kind of America_!) )
              ( Satellite (leaf 82) (rel2par elaboration-attribute) (text _!in which I believed , not as a Democrat or as a candidate , but as a citizen ._!) )
            )
          )
        )
        ( Nucleus (span 83 120) (rel2par joint-other)
          ( Nucleus (span 83 94) (rel2par span)
            ( Satellite (span 83 89) (rel2par context-background)
              ( Satellite (span 83 85) (rel2par adversative-concession)
                ( Nucleus (leaf 83) (rel2par joint-list) (text _!Today , in many ways , the world looks very different ,_!) )
                ( Nucleus (span 84 85) (rel2par joint-list)
                  ( Nucleus (leaf 84) (rel2par span) (text _!and the revolutionary change of pace is even more rapid_!) )
                  ( Satellite (leaf 85) (rel2par adversative-antithesis) (text _!than it has been in the past ._!) )
                )
              )
              ( Nucleus (span 86 89) (rel2par span)
                ( Nucleus (leaf 86) (rel2par span) (text _!But there has been no change in my concept of the goals_!) )
                ( Satellite (span 87 89) (rel2par elaboration-attribute)
                  ( Nucleus (leaf 87) (rel2par span) (text _!which this country must strive for_!) )
                  ( Satellite (span 88 89) (rel2par purpose-goal)
