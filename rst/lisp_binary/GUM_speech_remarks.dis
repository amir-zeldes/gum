              ( Nucleus (span 51 52) (rel2par same-unit)
                ( Satellite (leaf 51) (rel2par organization-preparation) (text _!what is more ,_!) )
                ( Nucleus (leaf 52) (rel2par span) (text _!has helped make the United States meet its responsibilities around the globe ._!) )
              )
            )
          )
        )
        ( Satellite (span 53 54) (rel2par causal-result)
          ( Nucleus (leaf 53) (rel2par joint-list) (text _!So what you do in this city counts all across the country ,_!) )
          ( Nucleus (leaf 54) (rel2par joint-list) (text _!and Philadelphia has sent the right men to the House of Representatives and to the Senate of the United States ._!) )
        )
