      ( Nucleus (span 73 120) (rel2par joint-other)
        ( Nucleus (span 73 82) (rel2par joint-other)
          ( Satellite (span 73 77) (rel2par adversative-antithesis)
            ( Nucleus (span 73 74) (rel2par joint-list)
              ( Satellite (leaf 73) (rel2par attribution-negative) (text _!I did not promise on that October night_!) )
              ( Nucleus (leaf 74) (rel2par span) (text _!that life would be easy in the Great Republic ._!) )
