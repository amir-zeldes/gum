( Root (span 1 84) 
  ( Satellite (span 1 4) (rel2par background)
    ( Satellite (span 1 2) (rel2par background)
      ( Nucleus (leaf 1) (rel2par span) (text _!This year has once again confirmed the importance of the United Nations as the truly indispensable multilateral forum_!) )
      ( Satellite (leaf 2) (rel2par elaboration) (text _!for addressing today ’s global problems ._!) )
    )
    ( Nucleus (span 3 4) (rel2par span)
      ( Nucleus (leaf 3) (rel2par span) (text _!The humanitarian crisis in the Horn of Africa , ecological disasters such as Fukushima , political developments like the Arab Spring and terrorist attacks such as the assault on the United Nations building in Abuja last August require concerted action by the United Nations and its Member States ._!) )
      ( Satellite (leaf 4) (rel2par evaluation) (text _!Ten years after 9/11 , joint action in the fight against terrorism continues to remain central on the multilateral agenda ._!) )
    )
  )
  ( Nucleus (span 5 84) (rel2par span)
    ( Nucleus (span 5 32) (rel2par joint)
      ( Nucleus (span 5 14) (rel2par joint)
        ( Nucleus (span 5 11) (rel2par span)
          ( Nucleus (span 5 7) (rel2par span)
            ( Nucleus (leaf 5) (rel2par span) (text _!In the past nine months , we have witnessed momentous changes in the Arab world_!) )
            ( Satellite (span 6 7) (rel2par elaboration)
              ( Nucleus (leaf 6) (rel2par span) (text _!that nobody expected_!) )
              ( Satellite (leaf 7) (rel2par circumstance) (text _!when we met one year ago ._!) )
            )
          )
          ( Satellite (span 8 11) (rel2par background)
            ( Nucleus (span 8 10) (rel2par span)
              ( Nucleus (leaf 8) (rel2par span) (text _!Pressure for change is driven by the wish of women and men_!) )
              ( Satellite (span 9 10) (rel2par elaboration)
                ( Nucleus (leaf 9) (rel2par joint) (text _!to choose their own fate_!) )
                ( Nucleus (leaf 10) (rel2par joint) (text _!and to improve their chances for a better life ._!) )
              )
            )
            ( Satellite (leaf 11) (rel2par evaluation) (text _!These are legitimate demands ._!) )
          )
        )
        ( Satellite (span 12 14) (rel2par elaboration)
          ( Nucleus (span 12 13) (rel2par span)
            ( Nucleus (leaf 12) (rel2par span) (text _!An overwhelming number of young people were at the forefront of this enormous tide ,_!) )
            ( Satellite (leaf 13) (rel2par elaboration) (text _!which started in Tunisia and Egypt ._!) )
          )
          ( Satellite (leaf 14) (rel2par evaluation) (text _!We should pay tribute to their courage ._!) )
        )
      )
      ( Nucleus (span 15 32) (rel2par joint)
        ( Nucleus (span 15 23) (rel2par joint)
          ( Satellite (span 15 16) (rel2par background)
            ( Nucleus (leaf 15) (rel2par contrast) (text _!In Libya , thousands lost their lives in the struggle for freedom and democracy ._!) )
            ( Nucleus (leaf 16) (rel2par contrast) (text _!In Syria , and to some extent in Yemen , the suppression is ongoing ._!) )
          )
          ( Nucleus (span 17 23) (rel2par span)
            ( Nucleus (span 17 18) (rel2par joint)
              ( Satellite (leaf 17) (rel2par justify) (text _!Austria strongly condemns the systematic human rights violations and the violence against peaceful demonstrators ._!) )
              ( Nucleus (leaf 18) (rel2par span) (text _!We strongly urge those responsible to immediately stop the bloodshed and to engage in meaningful dialogue and reforms ._!) )
            )
            ( Nucleus (span 19 23) (rel2par joint)
              ( Nucleus (leaf 19) (rel2par span) (text _!Austria supports the Libyan people in its struggle for freedom ._!) )
              ( Satellite (span 20 23) (rel2par evidence)
                ( Nucleus (span 20 22) (rel2par joint)
                  ( Nucleus (span 20 21) (rel2par same-unit)
                    ( Nucleus (leaf 20) (rel2par span) (text _!Over € 150 million_!) )
                    ( Satellite (leaf 21) (rel2par elaboration) (text _!blocked in Austrian bank accounts_!) )
                  )
                  ( Nucleus (leaf 22) (rel2par same-unit) (text _!has been de-frozen for humanitarian use ._!) )
                )
                ( Nucleus (leaf 23) (rel2par joint) (text _!Both the Austrian Government and private sector have provided medicine , relief supplies and desperately needed fuel to Libya ._!) )
              )
            )
          )
        )
        ( Nucleus (span 24 32) (rel2par joint)
          ( Nucleus (span 24 27) (rel2par span)
            ( Nucleus (span 24 25) (rel2par span)
              ( Nucleus (leaf 24) (rel2par span) (text _!The international community and the United Nations have to support and assist the transition process in the Arab world_!) )
              ( Satellite (leaf 25) (rel2par manner) (text _!as the United Nations does now in Libya ._!) )
            )
            ( Satellite (span 26 27) (rel2par concession)
              ( Nucleus (leaf 26) (rel2par joint) (text _!This period of change may last for some time_!) )
              ( Nucleus (leaf 27) (rel2par joint) (text _!and will be full of challenges ._!) )
            )
          )
          ( Satellite (span 28 32) (rel2par elaboration)
            ( Nucleus (span 28 29) (rel2par joint)
              ( Nucleus (leaf 28) (rel2par span) (text _!Austria welcomes all efforts_!) )
              ( Satellite (leaf 29) (rel2par elaboration) (text _!undertaken by Governments in the region for peaceful and credible change ._!) )
            )
            ( Nucleus (span 30 32) (rel2par joint)
              ( Nucleus (span 30 31) (rel2par span)
                ( Nucleus (leaf 30) (rel2par span) (text _!We encourage the newly empowered authorities to create a constitutional framework_!) )
                ( Satellite (leaf 31) (rel2par elaboration) (text _!based on democracy and human rights ,_!) )
              )
              ( Satellite (leaf 32) (rel2par purpose) (text _!in order to fulfil their mandate for democratic change responsibly and peacefully ._!) )
            )
          )
        )
      )
    )
    ( Nucleus (span 33 84) (rel2par joint)
      ( Nucleus (span 33 50) (rel2par joint)
        ( Nucleus (span 33 44) (rel2par joint)
          ( Nucleus (span 33 36) (rel2par span)
            ( Nucleus (span 33 34) (rel2par span)
              ( Satellite (leaf 33) (rel2par concession) (text _!The events in the Arab world notwithstanding ,_!) )
              ( Nucleus (leaf 34) (rel2par span) (text _!we must continue to focus on the Middle East peace process ._!) )
            )
            ( Satellite (span 35 36) (rel2par justify)
              ( Nucleus (leaf 35) (rel2par span) (text _!The Middle East took centre stage this week here at the General Assembly_!) )
              ( Satellite (leaf 36) (rel2par evaluation) (text _!— and rightly so ._!) )
            )
          )
          ( Satellite (span 37 44) (rel2par elaboration)
            ( Nucleus (leaf 37) (rel2par joint) (text _!We must build trust_!) )
            ( Nucleus (span 38 44) (rel2par joint)
              ( Nucleus (span 38 42) (rel2par span)
                ( Nucleus (leaf 38) (rel2par span) (text _!and we have to foster the belief among Israelis and Palestinians_!) )
                ( Satellite (span 39 42) (rel2par elaboration)
                  ( Nucleus (leaf 39) (rel2par span) (text _!that a negotiated settlement can be achieved_!) )
                  ( Satellite (span 40 42) (rel2par elaboration)
                    ( Nucleus (leaf 40) (rel2par span) (text _!— a sustainable solution_!) )
                    ( Satellite (span 41 42) (rel2par elaboration)
                      ( Nucleus (leaf 41) (rel2par span) (text _!based on two States_!) )
                      ( Satellite (leaf 42) (rel2par elaboration) (text _!living side by side in a secure and peaceful neighbourhood within mutually recognized borders ._!) )
                    )
                  )
                )
              )
              ( Satellite (span 43 44) (rel2par motivation)
                ( Satellite (leaf 43) (rel2par antithesis) (text _!We have no choice_!) )
                ( Nucleus (leaf 44) (rel2par span) (text _!but to return to direct negotiations between the two parties ._!) )
              )
            )
          )
        )
        ( Nucleus (span 45 50) (rel2par joint)
          ( Nucleus (span 45 47) (rel2par span)
            ( Nucleus (span 45 46) (rel2par joint)
              ( Nucleus (leaf 45) (rel2par span) (text _!The Quartet , in its statement yesterday , has shown a way_!) )
              ( Satellite (leaf 46) (rel2par elaboration) (text _!to do that_!) )
            )
            ( Nucleus (leaf 47) (rel2par joint) (text _!and has also proposed concrete timelines ._!) )
          )
          ( Satellite (span 48 50) (rel2par evaluation)
            ( Nucleus (leaf 48) (rel2par span) (text _!Austria fully supports the Quartet statement ._!) )
            ( Satellite (span 49 50) (rel2par justify)
              ( Nucleus (leaf 49) (rel2par span) (text _!There is no time_!) )
              ( Satellite (leaf 50) (rel2par elaboration) (text _!to lose ._!) )
            )
          )
        )
      )
      ( Nucleus (span 51 84) (rel2par joint)
        ( Nucleus (span 51 64) (rel2par joint)
          ( Nucleus (span 51 58) (rel2par joint)
            ( Nucleus (span 51 52) (rel2par span)
              ( Nucleus (leaf 51) (rel2par span) (text _!Austria welcomes President Al - Nasser ’s choice of theme for this year ’s general debate ._!) )
              ( Satellite (leaf 52) (rel2par evaluation) (text _!We appreciate Qatar ’s strong track record in that regard ._!) )
            )
            ( Satellite (span 53 58) (rel2par justify)
              ( Satellite (leaf 53) (rel2par attribution) (text _!Last year ’s tenth anniversary of the adoption of Security Council resolution 1325 ( 2000 ) on women , peace and security reminded us_!) )
              ( Nucleus (span 54 58) (rel2par span)
                ( Nucleus (span 54 57) (rel2par joint)
                  ( Nucleus (span 54 56) (rel2par same-unit)
                    ( Nucleus (span 54 55) (rel2par span)
                      ( Nucleus (leaf 54) (rel2par span) (text _!that the promise of women ’s full and equal participation in all efforts_!) )
                      ( Satellite (leaf 55) (rel2par elaboration) (text _!of maintaining peace and security ,_!) )
                    )
                    ( Satellite (leaf 56) (rel2par elaboration) (text _!including in mediation processes ,_!) )
                  )
                  ( Nucleus (leaf 57) (rel2par same-unit) (text _!has not yet come true_!) )
                )
                ( Nucleus (leaf 58) (rel2par joint) (text _!and that we all need to intensify our efforts ._!) )
              )
            )
          )
          ( Nucleus (span 59 64) (rel2par joint)
            ( Nucleus (span 59 62) (rel2par span)
              ( Satellite (leaf 59) (rel2par preparation) (text _!In that context , let me also mention an Austrian contribution to international mediation efforts ._!) )
              ( Nucleus (span 60 62) (rel2par span)
                ( Nucleus (span 60 61) (rel2par joint)
                  ( Satellite (leaf 60) (rel2par means) (text _!By inviting political decision - makers from both Khartoum and Juba to Vienna in recent years ,_!) )
                  ( Nucleus (leaf 61) (rel2par span) (text _!Austria has managed to offer the two sides a platform for discussion ._!) )
                )
                ( Nucleus (leaf 62) (rel2par joint) (text _!We are ready to continue to do so ._!) )
              )
            )
            ( Satellite (span 63 64) (rel2par elaboration)
              ( Satellite (leaf 63) (rel2par justify) (text _!The Sudan and South Sudan have a shared past_!) )
              ( Nucleus (leaf 64) (rel2par span) (text _!and must resolve their remaining problems peacefully ._!) )
            )
          )
        )
        ( Nucleus (span 65 84) (rel2par joint)
          ( Nucleus (span 65 69) (rel2par joint)
            ( Nucleus (span 65 67) (rel2par joint)
              ( Satellite (leaf 65) (rel2par background) (text _!The United Nations headquarters in Vienna serves as a dynamic hub for the promotion of peace , security and sustainable development ._!) )
              ( Nucleus (span 66 67) (rel2par span)
                ( Satellite (leaf 66) (rel2par attribution) (text _!I am proud to announce_!) )
                ( Nucleus (leaf 67) (rel2par span) (text _!that a liaison office of the United Nations Office for Disarmament Affairs will open in Vienna soon ._!) )
              )
            )
            ( Nucleus (span 68 69) (rel2par joint)
              ( Nucleus (leaf 68) (rel2par joint) (text _!Over the past year , the International Anti-Corruption Academy set up its headquarters near Vienna ,_!) )
              ( Nucleus (leaf 69) (rel2par joint) (text _!and the Vienna Centre for Disarmament and Non-Proliferation and an office of the International Peace Institute were established ._!) )
            )
          )
          ( Nucleus (span 70 84) (rel2par joint)
            ( Nucleus (span 70 73) (rel2par joint)
              ( Nucleus (span 70 72) (rel2par span)
                ( Nucleus (span 70 71) (rel2par span)
                  ( Nucleus (leaf 70) (rel2par span) (text _!Austria fully stands behind the campaign_!) )
                  ( Satellite (leaf 71) (rel2par elaboration) (text _!launched by the Secretary - General on Monday_!) )
                )
                ( Satellite (leaf 72) (rel2par elaboration) (text _!to achieve universal access to modern energy services ._!) )
              )
              ( Satellite (leaf 73) (rel2par elaboration) (text _!We support doubling the rate of improvement in energy efficiency and the share of renewable energy in the global energy mix by 2030 ._!) )
            )
            ( Nucleus (span 74 84) (rel2par joint)
              ( Nucleus (span 74 78) (rel2par joint)
                ( Nucleus (span 74 75) (rel2par span)
                  ( Nucleus (leaf 74) (rel2par span) (text _!Austria provides substantial support to the energy and environment efforts_!) )
                  ( Satellite (leaf 75) (rel2par elaboration) (text _!undertaken by the United Nations Industrial Development Organization ._!) )
                )
                ( Satellite (span 76 78) (rel2par evidence)
                  ( Nucleus (leaf 76) (rel2par span) (text _!Last June , we hosted the Vienna Energy Forum ,_!) )
                  ( Satellite (span 77 78) (rel2par elaboration)
                    ( Nucleus (leaf 77) (rel2par span) (text _!where about 1,400 participants discussed alternative energy concepts_!) )
                    ( Satellite (leaf 78) (rel2par elaboration) (text _!to reach the energy goals of the United Nations ._!) )
                  )
                )
              )
              ( Nucleus (span 79 84) (rel2par joint)
                ( Nucleus (leaf 79) (rel2par span) (text _!The Fukushima nuclear catastrophe should become a turning point in our approach to nuclear safety ._!) )
                ( Satellite (span 80 84) (rel2par motivation)
                  ( Nucleus (span 80 81) (rel2par joint)
                    ( Nucleus (leaf 80) (rel2par span) (text _!We owe it to future generations to make safety concerns come first_!) )
                    ( Satellite (leaf 81) (rel2par circumstance) (text _!when using nuclear power ._!) )
                  )
                  ( Nucleus (span 82 84) (rel2par joint)
                    ( Nucleus (leaf 82) (rel2par joint) (text _!Austria decided , in 1978 , to forgo the use of nuclear energy ._!) )
                    ( Nucleus (span 83 84) (rel2par joint)
                      ( Nucleus (leaf 83) (rel2par span) (text _!Some countries have recently adopted a similar course ,_!) )
                      ( Satellite (leaf 84) (rel2par evaluation) (text _!which we welcome ._!) )
                    )
                  )
                )
              )
            )
          )
        )
      )
    )
  )
)
