      ( Nucleus (leaf 2) (rel2par span) (text _!Announcing Decision_!) )
      ( Satellite (leaf 3) (rel2par purpose-attribute) (text _!To Resign the Office of President of the United States_!) )
    )
  )
  ( Nucleus (span 4 135) (rel2par span)
    ( Satellite (leaf 4) (rel2par context-circumstance) (text _!August 08 , 1974_!) )
    ( Nucleus (span 5 135) (rel2par span)
      ( Nucleus (span 5 56) (rel2par joint-other)
        ( Satellite (span 5 12) (rel2par organization-preparation)
          ( Nucleus (span 5 9) (rel2par joint-other)
            ( Satellite (leaf 5) (rel2par organization-phatic) (text _!Good evening :_!) )
            ( Nucleus (span 6 9) (rel2par span)
              ( Nucleus (leaf 6) (rel2par span) (text _!This is the 37th time_!) )
              ( Satellite (span 7 9) (rel2par elaboration-attribute)
                ( Nucleus (leaf 7) (rel2par span) (text _!I have spoken to you from this office ,_!) )
                ( Satellite (span 8 9) (rel2par elaboration-attribute)
                  ( Nucleus (leaf 8) (rel2par span) (text _!where so many decisions have been made_!) )
                  ( Satellite (leaf 9) (rel2par elaboration-attribute) (text _!that shaped the history of this Nation ._!) )
                )
              )
            )
          )
          ( Nucleus (span 10 12) (rel2par joint-other)
            ( Nucleus (leaf 10) (rel2par span) (text _!Each time I have done so_!) )
            ( Satellite (span 11 12) (rel2par purpose-goal)
              ( Nucleus (leaf 11) (rel2par span) (text _!to discuss with you some matter_!) )
              ( Satellite (leaf 12) (rel2par elaboration-attribute) (text _!that I believe affected the national interest ._!) )
            )
          )
        )
        ( Nucleus (span 13 56) (rel2par span)
          ( Satellite (span 13 29) (rel2par context-background)
            ( Nucleus (span 13 19) (rel2par adversative-contrast)
              ( Satellite (span 13 15) (rel2par context-background)
                ( Nucleus (span 13 14) (rel2par same-unit)
                  ( Nucleus (leaf 13) (rel2par span) (text _!In all the decisions_!) )
                  ( Satellite (leaf 14) (rel2par elaboration-attribute) (text _!I have made in my public life ,_!) )
                )
                ( Nucleus (leaf 15) (rel2par same-unit) (text _!I have always tried to do what was best for the Nation ._!) )
              )
              ( Nucleus (span 16 19) (rel2par span)
                ( Nucleus (leaf 16) (rel2par span) (text _!Throughout the long and difficult period of Watergate , I have felt it was my duty to persevere ,_!) )
                ( Satellite (span 17 19) (rel2par elaboration-additional)
                  ( Nucleus (leaf 17) (rel2par span) (text _!to make every possible effort_!) )
                  ( Satellite (span 18 19) (rel2par purpose-attribute)
                    ( Nucleus (leaf 18) (rel2par span) (text _!to complete the term of office_!) )
                    ( Satellite (leaf 19) (rel2par elaboration-attribute) (text _!to which you elected me ._!) )
                  )
                )
              )
            )
            ( Nucleus (span 20 29) (rel2par adversative-contrast)
              ( Satellite (span 20 21) (rel2par context-background)
                ( Nucleus (leaf 20) (rel2par span) (text _!In the past few days , however , it has become evident to me that I no longer have a strong enough political base in the Congress_!) )
                ( Satellite (leaf 21) (rel2par elaboration-attribute) (text _!to justify continuing that effort ._!) )
              )
              ( Nucleus (span 22 29) (rel2par span)
                ( Nucleus (span 22 25) (rel2par adversative-contrast)
                  ( Satellite (leaf 22) (rel2par contingency-condition) (text _!As long as there was such a base ,_!) )
                  ( Nucleus (span 23 25) (rel2par span)
                    ( Satellite (leaf 23) (rel2par attribution-positive) (text _!I felt strongly_!) )
                    ( Nucleus (span 24 25) (rel2par span)
                      ( Nucleus (leaf 24) (rel2par joint-list) (text _!that it was necessary to see the constitutional process through to its conclusion ,_!) )
                      ( Nucleus (leaf 25) (rel2par joint-list) (text _!that to do otherwise would be unfaithful to the spirit of that deliberately difficult process and a dangerously destabilizing precedent for the future ._!) )
                    )
                  )
                )
                ( Nucleus (span 26 29) (rel2par adversative-contrast)
                  ( Satellite (leaf 26) (rel2par attribution-positive) (text _!But with the disappearance of that base , I now believe_!) )
                  ( Nucleus (span 27 29) (rel2par span)
                    ( Nucleus (leaf 27) (rel2par joint-sequence) (text _!that the constitutional purpose has been served ,_!) )
                    ( Nucleus (span 28 29) (rel2par joint-sequence)
                      ( Nucleus (leaf 28) (rel2par span) (text _!and there is no longer a need_!) )
                      ( Satellite (leaf 29) (rel2par elaboration-attribute) (text _!for the process to be prolonged ._!) )
                    )
                  )
                )
              )
            )
          )
          ( Nucleus (span 30 56) (rel2par span)
            ( Satellite (span 30 51) (rel2par explanation-justify)
              ( Nucleus (span 30 34) (rel2par joint-other)
                ( Satellite (span 30 33) (rel2par adversative-concession)
                  ( Nucleus (span 30 32) (rel2par joint-list)
                    ( Nucleus (leaf 30) (rel2par span) (text _!I would have preferred to carry through to the finish ,_!) )
                    ( Satellite (span 31 32) (rel2par adversative-concession)
                      ( Nucleus (leaf 31) (rel2par span) (text _!whatever the personal agony_!) )
                      ( Satellite (leaf 32) (rel2par elaboration-attribute) (text _!it would have involved ,_!) )
                    )
                  )
                  ( Nucleus (leaf 33) (rel2par joint-list) (text _!and my family unanimously urged me to do so ._!) )
                )
                ( Nucleus (leaf 34) (rel2par span) (text _!But the interests of the Nation must always come before any personal considerations ._!) )
              )
              ( Nucleus (span 35 51) (rel2par joint-other)
                ( Nucleus (span 35 44) (rel2par joint-other)
                  ( Satellite (span 35 37) (rel2par attribution-positive)
                    ( Nucleus (span 35 36) (rel2par same-unit)
                      ( Nucleus (leaf 35) (rel2par span) (text _!From the discussions_!) )
                      ( Satellite (leaf 36) (rel2par elaboration-attribute) (text _!I have had with Congressional and other leaders ,_!) )
                    )
                    ( Nucleus (leaf 37) (rel2par same-unit) (text _!I have concluded_!) )
                  )
                  ( Nucleus (span 38 44) (rel2par span)
                    ( Nucleus (leaf 38) (rel2par same-unit) (text _!that_!) )
                    ( Nucleus (span 39 44) (rel2par same-unit)
                      ( Satellite (leaf 39) (rel2par causal-cause) (text _!because of the Watergate matter ,_!) )
                      ( Nucleus (span 40 44) (rel2par span)
                        ( Nucleus (leaf 40) (rel2par span) (text _!I might not have the support of the Congress_!) )
                        ( Satellite (span 41 44) (rel2par elaboration-attribute)
                          ( Nucleus (leaf 41) (rel2par span) (text _!that I would consider necessary_!) )
                          ( Satellite (span 42 44) (rel2par purpose-goal)
                            ( Nucleus (leaf 42) (rel2par joint-list) (text _!to back the very difficult decisions_!) )
                            ( Nucleus (span 43 44) (rel2par joint-list)
                              ( Nucleus (leaf 43) (rel2par span) (text _!and carry out the duties of this office in the way_!) )
                              ( Satellite (leaf 44) (rel2par elaboration-attribute) (text _!the interests of the Nation will require ._!) )
                            )
                          )
                        )
                      )
                    )
                  )
                )
                ( Nucleus (span 45 51) (rel2par joint-other)
                  ( Nucleus (span 45 48) (rel2par adversative-contrast)
                    ( Satellite (leaf 45) (rel2par explanation-justify) (text _!I have never been a quitter ._!) )
                    ( Nucleus (span 46 48) (rel2par span)
                      ( Nucleus (span 46 47) (rel2par same-unit)
                        ( Nucleus (leaf 46) (rel2par span) (text _!To leave office_!) )
                        ( Satellite (leaf 47) (rel2par context-circumstance) (text _!before my term is completed_!) )
                      )
                      ( Nucleus (leaf 48) (rel2par same-unit) (text _!is abhorrent to every instinct in my body ._!) )
                    )
                  )
                  ( Nucleus (span 49 51) (rel2par adversative-contrast)
                    ( Nucleus (leaf 49) (rel2par span) (text _!But as President , I must put the interests of America first ._!) )
                    ( Satellite (span 50 51) (rel2par elaboration-additional)
                      ( Nucleus (leaf 50) (rel2par span) (text _!America needs a full - time President and a full - time Congress , particularly at this time with problems_!) )
                      ( Satellite (leaf 51) (rel2par elaboration-attribute) (text _!we face at home and abroad ._!) )
                    )
                  )
                )
              )
            )
            ( Nucleus (span 52 56) (rel2par span)
              ( Nucleus (span 52 55) (rel2par joint-list)
                ( Satellite (span 52 54) (rel2par explanation-justify)
                  ( Nucleus (leaf 52) (rel2par span) (text _!To continue to fight through the months ahead for my personal vindication would almost totally absorb the time and attention of both the President and the Congress in a period_!) )
                  ( Satellite (span 53 54) (rel2par elaboration-attribute)
                    ( Nucleus (leaf 53) (rel2par span) (text _!when our entire focus should be on the great issues of peace abroad and prosperity_!) )
                    ( Satellite (leaf 54) (rel2par mode-manner) (text _!without inflation at home ._!) )
                  )
                )
                ( Nucleus (leaf 55) (rel2par span) (text _!Therefore , I shall resign the Presidency effective at noon tomorrow ._!) )
              )
              ( Nucleus (leaf 56) (rel2par joint-list) (text _!Vice President Ford will be sworn in as President at that hour in this office ._!) )
            )
          )
        )
      )
      ( Nucleus (span 57 135) (rel2par joint-other)
        ( Nucleus (span 57 109) (rel2par joint-other)
          ( Satellite (span 57 84) (rel2par explanation-justify)
            ( Nucleus (span 57 75) (rel2par joint-other)
              ( Nucleus (span 57 68) (rel2par joint-other)
                ( Nucleus (span 57 62) (rel2par adversative-contrast)
                  ( Satellite (span 57 59) (rel2par evaluation-comment)
                    ( Satellite (span 57 58) (rel2par context-circumstance)
                      ( Nucleus (leaf 57) (rel2par span) (text _!As I recall the high hopes for America_!) )
                      ( Satellite (leaf 58) (rel2par elaboration-attribute) (text _!with which we began this second term ,_!) )
                    )
                    ( Nucleus (leaf 59) (rel2par span) (text _!I feel a great sadness_!) )
                  )
                  ( Nucleus (span 60 62) (rel2par span)
                    ( Nucleus (leaf 60) (rel2par span) (text _!that I will not be here in this office_!) )
                    ( Satellite (span 61 62) (rel2par elaboration-additional)
                      ( Nucleus (leaf 61) (rel2par span) (text _!working on your behalf_!) )
                      ( Satellite (leaf 62) (rel2par purpose-goal) (text _!to achieve those hopes in the next 2 1/2 years ._!) )
                    )
                  )
                )
                ( Nucleus (span 63 68) (rel2par adversative-contrast)
                  ( Nucleus (leaf 63) (rel2par same-unit) (text _!But_!) )
                  ( Nucleus (span 64 68) (rel2par same-unit)
                    ( Satellite (leaf 64) (rel2par context-circumstance) (text _!in turning over direction of the Government to Vice President Ford ,_!) )
                    ( Nucleus (span 65 68) (rel2par span)
                      ( Satellite (span 65 67) (rel2par attribution-positive)
                        ( Nucleus (leaf 65) (rel2par span) (text _!I know ,_!) )
                        ( Satellite (span 66 67) (rel2par explanation-justify)
                          ( Nucleus (leaf 66) (rel2par span) (text _!as I told the Nation_!) )
                          ( Satellite (leaf 67) (rel2par context-circumstance) (text _!when I nominated him for that office 10 months ago ,_!) )
                        )
                      )
                      ( Nucleus (leaf 68) (rel2par span) (text _!that the leadership of America will be in good hands ._!) )
                    )
                  )
                )
              )
              ( Nucleus (span 69 75) (rel2par joint-other)
                ( Satellite (span 69 73) (rel2par context-background)
                  ( Satellite (leaf 69) (rel2par context-circumstance) (text _!In passing this office to the Vice President ,_!) )
                  ( Nucleus (span 70 73) (rel2par span)
                    ( Nucleus (span 70 71) (rel2par same-unit)
                      ( Nucleus (leaf 70) (rel2par span) (text _!I also do so with the profound sense of the weight of responsibility_!) )
                      ( Satellite (leaf 71) (rel2par elaboration-attribute) (text _!that will fall on his shoulders tomorrow_!) )
                    )
                    ( Nucleus (span 72 73) (rel2par same-unit)
                      ( Nucleus (leaf 72) (rel2par span) (text _!and , therefore , of the understanding , the patience , the cooperation_!) )
                      ( Satellite (leaf 73) (rel2par elaboration-attribute) (text _!he will need from all Americans ._!) )
                    )
                  )
                )
                ( Nucleus (span 74 75) (rel2par span)
                  ( Satellite (leaf 74) (rel2par context-circumstance) (text _!As he assumes that responsibility ,_!) )
                  ( Nucleus (leaf 75) (rel2par span) (text _!he will deserve the help and the support of all of us ._!) )
                )
              )
            )
            ( Nucleus (span 76 84) (rel2par joint-other)
              ( Satellite (span 76 80) (rel2par context-background)
                ( Nucleus (span 76 77) (rel2par joint-list)
                  ( Satellite (leaf 76) (rel2par context-circumstance) (text _!As we look to the future ,_!) )
                  ( Nucleus (leaf 77) (rel2par span) (text _!the first essential is to begin healing the wounds of this Nation ,_!) )
                )
                ( Nucleus (span 78 80) (rel2par joint-list)
                  ( Nucleus (leaf 78) (rel2par joint-list) (text _!to put the bitterness and divisions of the recent past behind us_!) )
                  ( Nucleus (span 79 80) (rel2par joint-list)
                    ( Nucleus (leaf 79) (rel2par span) (text _!and to rediscover those shared ideals_!) )
                    ( Satellite (leaf 80) (rel2par elaboration-attribute) (text _!that lie at the heart of our strength and unity as a great and as a free people ._!) )
                  )
                )
              )
              ( Nucleus (span 81 84) (rel2par span)
                ( Satellite (leaf 81) (rel2par mode-means) (text _!By taking this action ,_!) )
                ( Nucleus (span 82 84) (rel2par span)
                  ( Satellite (leaf 82) (rel2par attribution-positive) (text _!I hope_!) )
                  ( Nucleus (span 83 84) (rel2par span)
                    ( Nucleus (leaf 83) (rel2par span) (text _!that I will have hastened the start of that process of healing_!) )
                    ( Satellite (leaf 84) (rel2par elaboration-attribute) (text _!which is so desperately needed in America ._!) )
                  )
                )
              )
            )
          )
          ( Nucleus (span 85 109) (rel2par span)
            ( Satellite (span 85 92) (rel2par adversative-concession)
              ( Nucleus (span 85 87) (rel2par joint-other)
                ( Nucleus (leaf 85) (rel2par span) (text _!I regret deeply any injuries_!) )
                ( Satellite (span 86 87) (rel2par elaboration-attribute)
                  ( Nucleus (leaf 86) (rel2par span) (text _!that may have been done in the course of the events_!) )
                  ( Satellite (leaf 87) (rel2par elaboration-attribute) (text _!that led to this decision ._!) )
                )
              )
              ( Nucleus (span 88 92) (rel2par joint-other)
                ( Satellite (leaf 88) (rel2par attribution-positive) (text _!I would say only_!) )
                ( Nucleus (span 89 92) (rel2par span)
                  ( Nucleus (leaf 89) (rel2par same-unit) (text _!that_!) )
                  ( Nucleus (span 90 92) (rel2par same-unit)
                    ( Satellite (span 90 91) (rel2par contingency-condition)
                      ( Nucleus (leaf 90) (rel2par span) (text _!if some of my judgments were wrong_!) )
                      ( Satellite (leaf 91) (rel2par adversative-concession) (text _!-- and some were wrong --_!) )
                    )
                    ( Nucleus (leaf 92) (rel2par span) (text _!they were made in what I believed at the time to be the best interest of the Nation ._!) )
                  )
                )
              )
            )
            ( Nucleus (span 93 109) (rel2par span)
              ( Satellite (span 93 106) (rel2par explanation-motivation)
                ( Nucleus (span 93 99) (rel2par adversative-contrast)
                  ( Nucleus (span 93 94) (rel2par same-unit)
                    ( Nucleus (leaf 93) (rel2par span) (text _!To those_!) )
                    ( Satellite (leaf 94) (rel2par elaboration-attribute) (text _!who have stood with me during these past difficult months_!) )
                  )
                  ( Nucleus (span 95 99) (rel2par same-unit)
                    ( Nucleus (span 95 98) (rel2par same-unit)
                      ( Nucleus (leaf 95) (rel2par span) (text _!-- to my family , my friends , to many others_!) )
                      ( Satellite (span 96 98) (rel2par elaboration-attribute)
                        ( Nucleus (leaf 96) (rel2par span) (text _!who joined_!) )
                        ( Satellite (span 97 98) (rel2par purpose-goal)
                          ( Nucleus (leaf 97) (rel2par span) (text _!in supporting my cause_!) )
                          ( Satellite (leaf 98) (rel2par causal-cause) (text _!because they believed it was right --_!) )
                        )
                      )
                    )
                    ( Nucleus (leaf 99) (rel2par same-unit) (text _!I will be eternally grateful for your support ._!) )
                  )
                )
                ( Nucleus (span 100 106) (rel2par adversative-contrast)
                  ( Nucleus (span 100 101) (rel2par same-unit)
                    ( Nucleus (leaf 100) (rel2par span) (text _!And to those_!) )
                    ( Satellite (leaf 101) (rel2par elaboration-attribute) (text _!who have not felt able to give me your support ,_!) )
                  )
                  ( Nucleus (span 102 106) (rel2par same-unit)
                    ( Nucleus (span 102 104) (rel2par span)
                      ( Satellite (leaf 102) (rel2par attribution-positive) (text _!let me say_!) )
                      ( Nucleus (span 103 104) (rel2par span)
                        ( Nucleus (leaf 103) (rel2par span) (text _!I leave with no bitterness toward those_!) )
                        ( Satellite (leaf 104) (rel2par elaboration-attribute) (text _!who have opposed me ,_!) )
                      )
                    )
                    ( Satellite (span 105 106) (rel2par explanation-justify)
                      ( Nucleus (leaf 105) (rel2par span) (text _!because all of us , in the final analysis , have been concerned with the good of the country ,_!) )
                      ( Satellite (leaf 106) (rel2par adversative-concession) (text _!however our judgments might differ ._!) )
                    )
                  )
                )
              )
              ( Nucleus (span 107 109) (rel2par span)
                ( Nucleus (leaf 107) (rel2par span) (text _!So , let us all now join together_!) )
                ( Satellite (span 108 109) (rel2par purpose-goal)
                  ( Nucleus (leaf 108) (rel2par joint-list) (text _!in affirming that common commitment_!) )
                  ( Nucleus (leaf 109) (rel2par joint-list) (text _!and in helping our new President succeed for the benefit of all Americans ._!) )
                )
              )
            )
          )
        )
        ( Nucleus (span 110 135) (rel2par joint-other)
          ( Nucleus (span 110 129) (rel2par joint-list)
            ( Satellite (span 110 121) (rel2par context-background)
              ( Nucleus (span 110 119) (rel2par joint-list)
                ( Satellite (span 110 118) (rel2par adversative-concession)
                  ( Satellite (span 110 113) (rel2par organization-preparation)
                    ( Nucleus (span 110 111) (rel2par same-unit)
                      ( Nucleus (leaf 110) (rel2par span) (text _!I shall leave this office with regret_!) )
                      ( Satellite (leaf 111) (rel2par elaboration-attribute) (text _!at not completing my term ,_!) )
                    )
                    ( Nucleus (span 112 113) (rel2par same-unit)
                      ( Nucleus (leaf 112) (rel2par span) (text _!but with gratitude for the privilege_!) )
                      ( Satellite (leaf 113) (rel2par elaboration-attribute) (text _!of serving as your President for the past 5 1/2 years ._!) )
                    )
                  )
                  ( Nucleus (span 114 118) (rel2par span)
                    ( Satellite (leaf 114) (rel2par evaluation-comment) (text _!These years have been a momentous time in the history of our Nation and the world ._!) )
                    ( Nucleus (span 115 118) (rel2par span)
                      ( Nucleus (span 115 116) (rel2par same-unit)
                        ( Nucleus (leaf 115) (rel2par span) (text _!They have been a time of achievement_!) )
                        ( Satellite (leaf 116) (rel2par elaboration-attribute) (text _!in which we can all be proud ,_!) )
                      )
                      ( Nucleus (span 117 118) (rel2par same-unit)
                        ( Nucleus (leaf 117) (rel2par span) (text _!achievements_!) )
                        ( Satellite (leaf 118) (rel2par elaboration-attribute) (text _!that represent the shared efforts of the Administration , the Congress , and the people ._!) )
                      )
                    )
                  )
                )
                ( Nucleus (leaf 119) (rel2par span) (text _!But the challenges ahead are equally great ,_!) )
              )
              ( Nucleus (span 120 121) (rel2par joint-list)
                ( Nucleus (leaf 120) (rel2par span) (text _!and they , too , will require the support and the efforts of the Congress and the people_!) )
                ( Satellite (leaf 121) (rel2par elaboration-attribute) (text _!working in cooperation with the new Administration ._!) )
              )
            )
            ( Nucleus (span 122 129) (rel2par span)
              ( Satellite (span 122 125) (rel2par context-background)
                ( Satellite (leaf 122) (rel2par adversative-concession) (text _!We have ended America 's longest war ,_!) )
                ( Nucleus (span 123 125) (rel2par span)
                  ( Nucleus (span 123 124) (rel2par same-unit)
                    ( Nucleus (leaf 123) (rel2par span) (text _!but in the work_!) )
                    ( Satellite (leaf 124) (rel2par elaboration-attribute) (text _!of securing a lasting peace in the world ,_!) )
                  )
                  ( Nucleus (leaf 125) (rel2par same-unit) (text _!the goals ahead are even more far - reaching and more difficult ._!) )
                )
              )
              ( Nucleus (span 126 129) (rel2par span)
                ( Nucleus (leaf 126) (rel2par span) (text _!We must complete a structure of peace_!) )
                ( Satellite (span 127 129) (rel2par purpose-goal)
                  ( Satellite (leaf 127) (rel2par attribution-positive) (text _!so that it will be said of this generation , our generation of Americans , by the people of all nations ,_!) )
                  ( Nucleus (span 128 129) (rel2par span)
                    ( Nucleus (leaf 128) (rel2par joint-list) (text _!not only that we ended one war_!) )
                    ( Nucleus (leaf 129) (rel2par joint-list) (text _!but that we prevented future wars ._!) )
                  )
                )
              )
            )
          )
          ( Nucleus (span 130 135) (rel2par joint-list)
            ( Nucleus (span 130 131) (rel2par joint-sequence)
              ( Nucleus (leaf 130) (rel2par span) (text _!We have unlocked the doors_!) )
              ( Satellite (leaf 131) (rel2par elaboration-attribute) (text _!that for a quarter of a century stood between the United States and the People 's Republic of China ._!) )
            )
            ( Nucleus (span 132 135) (rel2par joint-sequence)
              ( Satellite (leaf 132) (rel2par attribution-positive) (text _!We must now ensure_!) )
              ( Nucleus (span 133 135) (rel2par span)
                ( Nucleus (span 133 134) (rel2par same-unit)
                  ( Nucleus (leaf 133) (rel2par span) (text _!that the one quarter of the world 's people_!) )
                  ( Satellite (leaf 134) (rel2par elaboration-attribute) (text _!who live in the People 's Republic of China_!) )
                )
                ( Nucleus (leaf 135) (rel2par same-unit) (text _!will be and remain not our enemies , but our friends ._!) )
              )
            )
          )
        )
      )
    )
  )
