		<segment id="117" parent="235" relname="attribution-positive">Katie Benner .</segment>
		<segment id="118" parent="235" relname="span">“ Inside the Hotel Industry ’s Plan</segment>
		<segment id="119" parent="118" relname="purpose-attribute">to Combat Airbnb . ”</segment>
