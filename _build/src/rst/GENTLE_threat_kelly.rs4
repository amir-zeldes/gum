		<segment id="51" parent="78" relname="same-unit">so</segment>
		<segment id="52" parent="53" relname="contingency-condition">if you do n't want to go to jail</segment>
		<segment id="53" parent="77" relname="span">get me the fuck out of here</segment>
		<segment id="54" parent="110" relname="span">or maybe your fucking Mom can help .</segment>
		<segment id="55" parent="54" relname="organization-phatic">Ha Ha .</segment>
