		<segment id="12" parent="144" relname="organization-heading">Etymology</segment>
		<segment id="13" parent="141" relname="span">From Middle English nexte , nexste , nixte , from Old English nīehsta , nīehste , etc. , inflected forms of nīehst</segment>
		<segment id="14" parent="13" relname="elaboration-attribute">( “ nearest , next ” ) ,</segment>
		<segment id="15" parent="142" relname="span">superlative form of nēah</segment>
		<segment id="16" parent="15" relname="elaboration-attribute">( “ nigh , near ” ) ,</segment>
		<segment id="17" parent="143" relname="span">corresponding to Proto-Germanic *nēhwist</segment>
		<segment id="18" parent="17" relname="elaboration-attribute">( “ nearest , closest ” ) ;</segment>
		<segment id="19" parent="144" relname="joint-list">equivalent to nigh + -est .</segment>
		<segment id="20" parent="146" relname="span">Cognate with Saterland Frisian naist</segment>
		<segment id="21" parent="20" relname="elaboration-attribute">( “ next ” ) ,</segment>
